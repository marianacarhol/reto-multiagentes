{
  "name": "my-tool",
  "version": "1.0.0",
  "description": "room service y mantenimiento",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npm run clean && tsc",
    "build:rollup": "npm run clean && rollup -c",
    "build:webpack": "npm run clean && webpack --mode production",
    "build:vite": "npm run clean && vite build",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "clean": "rimraf dist coverage .nyc_output .tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src --ext .ts,.js --fix",
    "lint:check": "eslint src --ext .ts,.js",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint:check && npm run format:check && npm run test:ci",
    "prepare": "npm run build",
    "prepack": "npm run validate",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "schema": "curl -s http://localhost:3000/schema | jq '.' || echo 'Server not running or jq not installed'",
    "deploy": "echo \"Deployment not configured yet. See README.md for deployment options.\"",
    "docker:build": "docker build -t my-tool .",
    "docker:run": "docker run -p 3000:3000 my-tool",
    "update-deps": "npx npm-check-updates -u && npm install",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "ai-spine",
    "tool",
    "my-tool"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-spine/tools": "^1.0.0",
    "@supabase/supabase-js": "^2.57.0",
    "dotenv": "^17.2.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.8",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-unicorn": "^49.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
